				<script charset="UTF-8">js/forms.js</script>
			</block>
			<block type="Page/html_body" name="html_body" html_class="cuerpo">
				<wrapper html_class="wrapper">
					<wrapper html_class="head">
						<wrapper html_class="head_center">
							<template name="head_logo" template="page/head/logo.phtml" />
							<wrapper html_class="head_content">
								<block name="head_content">
									<template template="page/head/search.phtml" />
									<template in="usuario_no_logeado" template="page/head/usermenu_unlogged.phtml" />
									<template in="usuario_logeado" template="page/head/usermenu_logged.phtml" />
								</block>
							</wrapper>
						</wrapper>
					</wrapper>
					<wrapper html_class="contenedor_mensajes">
						<block name="message_container" type="MessageContainer">
							<action method="requestMessageContainerTo">
								<class>Core_App</class>
								<method>getMessageContainer</method>
							</action>
						</block>
					</wrapper>
					<wrapper html_class="middle">
						<wrapper html_class="left" name="left">
							<template template="page/left/title.phtml" />
						</wrapper>
						<wrapper html_class="content">
							<block name="contenido">
							</block>
						</wrapper>
						<wrapper html_class="bottom">
						</wrapper>
					</wrapper>
					<wrapper html_class="footer">
						<template template="page/footer/links.phtml" />
					</wrapper>
				</wrapper>
			</block>
		</block>
	</default2>
