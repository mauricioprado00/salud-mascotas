	<default2>
		<block type="Page/html_document" name="html_document">
			<block compress_scripts="0" type="Page/html_head" name="html_head" title="SaludMascotas">
			
				<css name="saludmascotas_css_estilos">css/styles.css</css>
				<script charset="utf8">jquery-ui-1.8.1.custom/development-bundle/jquery-1.4.2.js</script>
				<script charset="utf8">prototype/prototype.js</script>
				<script charset="utf8">js/prototype.function.js</script>
